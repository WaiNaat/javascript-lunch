{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": ["error", "windows"],
        "quotes": ["error", "double"],
        "import/extensions": 0,
        "no-param-reassign": ["error", { "props": false }],
        "import/no-unresolved": 0
    }
}
